<mvc:View
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc"
   controllerName="walkthrough.controller.InvoiceDetail">
   <VBox>
      <Text text="Título: {invoice>title}" />
      <Text text="ID: {invoice>id}" />
      <Text text="Usuário: {invoice>userId}" />
      <CheckBox text="Concluído" selected="{invoice>completed}" enabled="false"/>
   </VBox>
</mvc:View>